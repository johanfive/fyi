{
  "name": "fyi",
  "displayName": "FYI",
  "description": "Nobody reads READMEs. Motion attracts attention. This extension helps repo maintainers create notifications that will pop up next time someone opens their project in VS Code.",
  "version": "0.0.1",
  "author": "johanFive",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ "onStartupFinished" ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fyi.resetDismissedNotifications",
        "title": "FYI: Reset Dismissed Notifications"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
